{
  "name": "xryuseix.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:pages": "npx @cloudflare/next-on-pages",
    "cf-typegen": "yarn wrangler types --env-interface CloudflareEnv env.d.ts",
    "deploy": "NODE_ENV=production deno task build:pages && wrangler pages deploy",
    "dev": "next dev",
    "format": "deno fmt src public",
    "lint": "deno lint src",
    "live2d:build": "pushd ./CubismWebSamples && deno task build && popd && deno task live2d:copy",
    "live2d:copy": "cp -r ./CubismWebSamples/dist/assets/index-*.js ./public/bundle.js",
    "logging": "wrangler pages deployment tail",
    "preview": "deno task build:pages && wrangler pages dev",
    "start": "next start",
    "test": "deno test"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.2.2",
    "@phosphor-icons/react": "^2.1.7",
    "@rewind-ui/core": "^0.19.11",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/react": "^18.3.9",
    "autoprefixer": "^10.4.19",
    "class-variance-authority": "^0.7.0",
    "hono": "^4.4.13",
    "next": "14.1.0",
    "next-auth": "^5.0.0-beta.19",
    "openai": "^4.53.0",
    "postcss": "^8.4.39",
    "react": "^18.3.1",
    "react-dom": "^18",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.4.0",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.12.1",
    "@cloudflare/workers-types": "^4.20240701.0",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/webspeechapi": "^0.0.29",
    "@vitejs/plugin-react": "^4.3.1",
    "jsdom": "^24.1.1",
    "typescript": "^5",
    "vercel": "^35.2.1",
    "vitest": "^2.0.4",
    "wrangler": "^3.64.0"
  }
}
